package com.theicecreambear.engine;

import javax.swing.JFrame;

public class GameEngine {
	public static boolean running = true;
	public JFrame frame;
	
	public GameEngine() {
		initialize();
		run();
	}
	
	public void initialize() {
		
	}
	
	public void run() {
		long time = System.nanoTime();
		long deltaTime;
		
		while(running) {
			deltaTime = System.nanoTime() - time;
			
		}
	}
}
